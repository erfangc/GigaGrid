{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "es6"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "declarationDir": "lib",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "outDir": "build"
  },
  "exclude": [
    "dist",
    "node_modules",
    "lib",
    "index.d.ts"
  ]
}
